{
  "name": "winsleague",
  "version": "2.0.0",
  "description": "Wins League",
  "readme": "README",
  "repository": {
    "type": "git",
    "url": "https://github.com/winsleague/winsleague.git"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "faker": "3.1.0",
    "lodash": "4.15.0",
    "loggly": "1.1.0",
    "meteor-node-stubs": "0.2.3",
    "moment": "2.14.1",
    "moment-timezone": "0.5.5",
    "spacejam": "https://github.com/serut/spacejam/tarball/windows-suppport-rc4",
    "winston": "2.2.0",
    "winston-loggly-bulk": "1.3.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-import-resolver-meteor": "0.3.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-meteor": "4.0.0",
    "eslint-plugin-react": "6.0.0",
    "shell-source": "1.1.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "spacejam": "https://github.com/serut/spacejam/tarball/windows-suppport-rc4"
  },
  "scripts": {
    "start": "meteor",
    "start-coverage": "COVERAGE=1 COVERAGE_VERBOSE=1 COVERAGE_APP_FOLDER=${PWD/#$HOME/~}/ meteor",
    "pretest": "echo 'npm run lint --silent'",
    "test": "meteor test --once --driver-package dispatch:mocha-phantomjs --port 3100",
    "test-app": "meteor test --full-app --once --driver-package dispatch:mocha-phantomjs --port 3100",
    "test-watch": "meteor test --driver-package practicalmeteor:mocha --port 3100",
    "test-app-watch": "meteor test --full-app --driver-package practicalmeteor:mocha --port 3100",
    "test-watch-terminal": "TEST_WATCH=1 meteor test --driver-package dispatch:mocha-phantomjs --port 3100",
    "test-app-watch-terminal": "TEST_WATCH=1 meteor test --full-app --driver-package dispatch:mocha-phantomjs --port 3100",
    "coverage": "meteor npm run coverage-app-unit && meteor npm run coverage-app-full",
    "coverage-app-unit": "spacejam --driver-package practicalmeteor:mocha-console-runner test --coverage 'out_lcovonly out_coverage out_html' --settings settings.coverage.json",
    "coverage-app-full": "spacejam --driver-package practicalmeteor:mocha-console-runner test --full-app --coverage 'out_lcovonly out_coverage out_html in_coverage'  --settings settings.coverage.json",
    "coverage-app-unit-watch": "meteor --driver-package practicalmeteor:mocha test --settings settings.coverage.json",
    "coverage-app-full-watch": "meteor --driver-package practicalmeteor:mocha test --full-app --settings settings.coverage.json",
    "lint": "eslint ."
  },
  "author": "",
  "license": "GPL-2.0"
}
