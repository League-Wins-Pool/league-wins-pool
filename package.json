    "simpl-schema": "1.7.3",
    "store": "2.0.12",
    "winston": "2.3.0",
    "winston-loggly-bulk": "1.3.4"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "1.0.5",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "chai-jquery": "2.1.0",
    "chimpy": "3.0.4",
    "chromedriver": "79.0.0",
    "eslint": "6.8.0",
